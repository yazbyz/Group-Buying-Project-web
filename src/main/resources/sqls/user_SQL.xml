<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user">

	<select id="selectSuggestRate"  parameterType="int" resultType="commentDto">
   		SELECT suggest_rate
		FROM user
		WHERE user_id = #{userId};
	</select>
	
	<select id="selectRequestRate"  parameterType="int" resultType="commentDto">
   		SELECT request_rate
		FROM user
		WHERE user_id = #{userId};
	</select>
	
</mapper>